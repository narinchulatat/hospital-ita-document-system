# Hospital ITA Document System .gitignore

# Uploaded files
uploads/documents/*
!uploads/documents/.gitkeep

# Backup files
backups/*
!backups/.gitkeep

# Temporary files
temp/*
!temp/.gitkeep

# Configuration files with sensitive data
config/database.php

# Log files
*.log

# IDE and Editor files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# PHP specific
vendor/
composer.lock

# Node modules (if using npm for frontend assets)
node_modules/
npm-debug.log

# Cache and session files
cache/
sessions/

# Environment files
.env
.env.local
.env.production

# Archive files
*.zip
*.tar.gz
*.rar

# Database dumps
*.sql
!database/ita_hospital_db.sql

# Error logs
error.log
access.log